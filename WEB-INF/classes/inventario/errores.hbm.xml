<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
     <class name="inventario.errores" >
		  <id name="cdError" type="int">
		   <column name="coderror"/>
		   <generator class="assigned"/>
		  </id>
          <property name="dsError" type="java.lang.String">
                <column name="deserror" />
          </property>
     </class>     
 <sql-query name="borrar_usuario_horas" >
 <return alias="borrar_usuario_horas" class="inventario.errores" >
   <return-property name="cdError" column="coderror" />
   <return-property name="dsError" column="deserror" />
  </return>
  <![CDATA[execute dbo.borrar_usuario_horas]]>
 </sql-query>
</hibernate-mapping>
