<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="inventario.Apps" table="INV03">
        <composite-id>
  			<key-property name="seriesist" column="seriesist" type="java.lang.String"></key-property>
  			<key-property name="idProgram" column="id_programa" type="int"></key-property>
		</composite-id>
        <property name="dsProgram" type="java.lang.String">
            <column name="ds_programa" />
        </property>
        <property name="path" type="java.lang.String">
            <column name="path" />
        </property>
        <property name="version" type="java.lang.String">
            <column name="version" />
        </property>
        <property name="licencia" type="java.lang.String">
            <column name="licencia" />
        </property>
         <property name="fechaModi" type="timestamp">
            <column name="fecha_modi" />
        </property>
        <property name="usuarioModi" type="java.lang.String">
            <column name="usuario_modi" />
        </property>                
    </class>
</hibernate-mapping>